isbn = {0899-7667},
issn = {1530888X},
journal = {Neural Computation},
number = {28},
pages = {2474--2504},
pmid = {25602775},
title = {{Continuous Online Sequence Learning with an Unsupervised Neural Network Model}},
url = {http://arxiv.org/abs/1803.01446},
volume = {2504},
year = {2018}
}
