pages = {975--983},
publisher = {Elsevier},
title = {{Error detection and diagnosis for fault tolerance in distributed systems}},
url = {https://www-sciencedirect-com.proxy.lnu.se/science/article/pii/S095058499700058X},
volume = {39},
year = {1998}
}
@article{Fiaschetti2011,
abstract = {In this paper a semantic approach is presented to model and control Security, Privacy and Dependability (SPD) in complex interconnected environment composed by heterogeneous Embedded Systems. Usually, only the individual properties are locally considered to obtain desired functionalities and this could result in sub-optimal solutions. With the use of modern semantic technologies (like OWL or reasoning engines) it is possible to model not only the individual parameters but also the relations between the different (and dynamically changing) parts of the systems, thus providing enriched knowledge and more useful information that could feed control algorithms. The model presented in this paper is based on the results obtained during the first phase of the pSHIELD 1 project (conceived and lead by Finmeccanica) and it is focused on a concrete application coming from a critical scenario in railway environment: the monitoring of freight trains transporting hazardous material.},
author = {Fiaschetti, Andrea and Lavorato, Francesco and Suraci, Vincenzo and Palo, Andi and Taglialatela, Andrea and Morgagni, Andrea and Baldelli, Renato and Flammini, Francesco},
file = {:C$\backslash$:/Users/prasa.DESKTOP-L7LFCK6/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Fiaschetti et al. - 2011 - On the Use of Semantic Technologies to Model and Control Security, Privacy and Dependability in Complex Syste.pdf:pdf},
journal = {LNCS},
keywords = {Common Criteria,Dependability,Model,Ontology,Privacy,Security},
pages = {467--479},
title = {{On the Use of Semantic Technologies to Model and Control Security, Privacy and Dependability in Complex Systems}},
